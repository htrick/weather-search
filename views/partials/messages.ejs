<% if (typeof errors != "undefined") { %>
    <% errors.forEach((error) => { %>
        <div class="error">
            <%= error.msg %>
        </div>
    <% }); %>
<% } %>

<% if (success != "") { %>
    <div class="success">
        <%= success %>
    </div>
<% } %>

<% if (error != "") { %>
    <div class="error">
        <%= error %>
    </div>
<% } %>
